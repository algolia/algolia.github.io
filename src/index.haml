  %meta{charset: "UTF-8"}
  %title Algolia Community
  %link{href: "app.css", rel: "stylesheet", type: "text/css"}

  %link{rel: "apple-touch-icon", sizes: "57x57", href: "/img/icons/apple-icon-57x57.png"}
